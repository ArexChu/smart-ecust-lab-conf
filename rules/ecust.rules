rule "VoiceControl" 

when 
    Item VoiceCommand received command 
then 
	var String command = VoiceCommand.state.toString.toLowerCase
	logInfo("Voice.Rec","VoiceCommand received "+command)
	if (command.contains("on")) {
		Light_FF_r.sendCommand(ON)
	}
end

